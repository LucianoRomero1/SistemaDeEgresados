    padding:90px 15px;
}
.navbar-expand-lg .navbar-nav.side-nav{
    flex-direction: column;
}
.card{
    margin-bottom: 15px;
    border-radius:0; 
    box-shadow: 0 3px 5px rgba(0,0,0,.1); 
}
.header-top{
    box-shadow: 0 3px 5px rgba(0,0,0,.1)
}
.leftmenutrigger{
    display: none;
}
@media(min-width:992px) {

    .leftmenutrigger{
        display: block;
        margin: 7px 20px 4px 0;
        cursor: pointer;
    }
    #wrapper{
        padding: 90px 15px 15px 15px;
    }
        
    .navbar-nav.side-nav.open {
        left:0;
    }
    .navbar-nav.side-nav{
        background: #fcfbfb;
        box-shadow: 2px 1px 2px rgba(0,0,0,.1);
        position:fixed;
        top:56px;
        flex-direction: column!important;
        left:-220px;
        width:200px;
        overflow-y:auto;
        bottom:0;
        overflow-x:hidden;
        padding-bottom:40px;
    }
}
